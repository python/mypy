[case testLoop_BorrowedArgument]
def f(a: int) -> int:
    sum = 0
    i = 0
    while i <= a:
        sum = sum + i
        i = i + 1
    return sum
[out]
L0:
    sum = 0
    i = 0
L1:
    if i <= a goto L2 else goto L3 :: int
L2:
    sum = sum + i :: int
    r0 = 1
    i = i + r0 :: int
    goto L1
L3:
    return sum

