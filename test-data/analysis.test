-- Test cases for data flow analysis.

[case testSimple_MaybeDefined]
def f(a: int) -> None:
    x = 1
    if x == a:
        y = 1
    else:
        z = 1
[out]
L0:
    x = 1
    if x == a goto L1 else goto L2 :: int
L1:
    y = 1
    goto L3
L2:
    z = 1
L3:
    r0 = None
    return r0

(0, 0)   {a}                     {a, x}
(0, 1)   {a, x}                  {a, x}
(1, 0)   {a, x}                  {a, x, y}
(1, 1)   {a, x, y}               {a, x, y}
(2, 0)   {a, x}                  {a, x, z}
(2, 1)   {a, x, z}               {a, x, z}
(3, 0)   {a, x, y, z}            {a, x, y, z, r0}
(3, 1)   {a, x, y, z, r0}        {a, x, y, z, r0}

