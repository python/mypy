[case testLoop_MaybeDefined]
def f(a: int) -> None:
    while a < a:
        while a < a:
            y = a
        x = a
[out]
L0:
L1:
    if a < a goto L2 else goto L6 :: int
L2:
L3:
    if a < a goto L4 else goto L5 :: int
L4:
    y = a
    goto L3
L5:
    x = a
    goto L1
L6:
    r0 = None
    return r0

