[case testFunctionDecorator]
from typing import TypeVar, cast
ftype = TypeVar('ftype')
def logged(f: ftype) -> ftype:
    def g(*args, **kwargs):
        print('enter', f.__name__)
        r = f(*args, **kwargs)
        print('exit', f.__name__)
        return r
    return cast(ftype, g)

@logged
def foo(s: str) -> str:
    print('foo', s)
    return s + '!'

print(foo('y'))
print(foo('x'))
[out]
enter foo
foo y
exit foo
y!
enter foo
foo x
exit foo
x!

