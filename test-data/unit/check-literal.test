reveal_type(b_bytes_wrapper_alias)    # N: Revealed type is "__main__.Wrap[Literal['foo']]"
reveal_type(c_bytes_wrapper_alias)    # N: Revealed type is "__main__.Wrap[Literal['foo']]"
