[case testUnreachableStatementAfterRaise]
def f(x: bool) -> int:
    if x:
        raise ValueError()
        print('hello')
    return 2
[out]
def f(x):
    x :: bool
    r0 :: object
    r1 :: str
    r2, r3 :: object
L0:
    if x goto L1 else goto L2 :: bool
L1:
    r0 = builtins :: module
    r1 = 'ValueError'
    r2 = CPyObject_GetAttr(r0, r1)
    r3 = PyObject_Vectorcall(r2, 0, 0, 0)
    CPy_Raise(r3)
    unreachable
L2:
    return 4

