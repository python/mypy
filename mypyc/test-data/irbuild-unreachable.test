[case testUnreachableStatementAfterReturn]
def f(x: bool) -> int:
    if x:
        return 1
        f(False)
    return 2
[out]
def f(x):
    x :: bool
L0:
    if x goto L1 else goto L2 :: bool
L1:
    return 2
L2:
    return 4

