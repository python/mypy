    r2 = CPyObject_GetAttr(r0, r1)
    r3 = cast(str, r2)
    r4 = 'x'
    r5 = CPyStr_EqualLiteral(r3, r4, 1)
    if r5 goto L2 else goto L1 :: bool
L1:
    r6 = r5
    goto L3
L2:
    r7 = raise RuntimeError('mypyc internal error: should be unreachable')
    r8 = box(None, 1)
    r9 = unbox(bool, r8)
    r6 = r9
L3:
    y = r6
    return 1

