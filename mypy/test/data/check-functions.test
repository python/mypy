f = Undefined # type: Function[[B], A]
g = Undefined # type: Function[[A], A]
h = Undefined # type: Function[[B], B]  # subtype of f
g = f  # E: Incompatible types in assignment (expression has type "B", variable has type "A")
g = h  # E: Incompatible types in assignment (expression has type "B", variable has type "A")
h = f  # E: Incompatible types in assignment (expression has type "A", variable has type "B")
h = g  # E: Incompatible types in assignment (expression has type "A", variable has type "B")
f = g
f = h
f = f
g = g
h = h

class A: pass
class B(A): pass

[case testFunctionTypeCompatibilityWithOtherTypes]
