[case testFunctionTypesWithOverloads]
from typing import Undefined, Function, overload, disjointclass
f = Undefined # type: Function[[AA], A]
g = Undefined # type: Function[[B], B]
h = Undefined # type: Function[[A], AA]

h = i  # E: Incompatible types in assignment (expression has type , variable has type )
f = j

f = i
g = i
g = j

class A: pass
class AA(A): pass

@disjointclass(A)
class B: pass

@overload
def i(x: AA) -> A:
    pass
@overload
def i(x: B) -> B:
    pass

@overload
def j(x: B) -> B:
    pass
@overload
def j(x: A) -> AA:
    pass

[case testOverloadWithThreeItems]
from typing import Undefined, Function, overload, disjointclass
g1 = Undefined # type: Function[[A], A]
g2 = Undefined # type: Function[[B], B]
g3 = Undefined # type: Function[[C], C]
g4 = Undefined # type: Function[[A], B]
a, b, c = Undefined, Undefined, Undefined # type: (A, B, C)

b = f(a)  # E: Incompatible types in assignment (expression has type "A", variable has type "B")
a = f(b)  # E: Incompatible types in assignment (expression has type "B", variable has type "A")
b = f(c)  # E: Incompatible types in assignment (expression has type "C", variable has type "B")
g4 = f    # E: Incompatible types in assignment (expression has type , variable has type )

g1 = f
g2 = f
g3 = f
a = f(a)
b = f(b)
c = f(c)

class A: pass

@disjointclass(A)
class B: pass

@disjointclass(A)
@disjointclass(B)
class C: pass

@overload
def f(x: A) -> A: pass
@overload
def f(x: B) -> B: pass
@overload
def f(x: C) -> C: pass


